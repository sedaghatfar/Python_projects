#Pig Latin translator
#codecademy
 
pyg = 'ay'
 
original = raw_input('Enter a word:')
 
if len(original) > 0 and original.isalpha():
  word = original.lower()
	first = word[0]
		
	if first == ('a' or 'e' or 'i' or 'o' or 'u'):
		new_word = word + pyg
		print new_word
	else:
		j = len(word)
		new_word = word[1:j] + first + pyg
		print new_word
	
else:
	print "empty"
